{
  "name": "@rjsebening/n8n-nodes-easybill-api",
  "version": "0.1.20",
  "description": "n8n node for the Easybill REST API, allowing management of documents, customers, positions, and more.",
  "type": "commonjs",
  "keywords": [
    "n8n-community-node-package",
    "easybill"
  ],
  "license": "MIT",
  "homepage": "https://github.com/rjsebening/n8n-nodes-easybill-api",
  "author": {
    "name": "JÃ¶rg Sebening",
    "email": "dev@joerg-sebening.com"
  },
  "bugs": {
    "url": "https://github.com/rjsebening/n8n-nodes-easybill-api/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjsebening/n8n-nodes-easybill-api.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "!dist/package.json",
    "!dist/**/*.map",
    "!dist/tsconfig.tsbuildinfo",
    "README.md",
    "LICENSE.md"
  ],
