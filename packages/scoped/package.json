    ],
    "credentials": [
      "dist/credentials/EasybillApiBasicApi.credentials.js",
      "dist/credentials/EasybillApiBearerApi.credentials.js"
    ]
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "publishConfig": {
    "access": "public"
  }
